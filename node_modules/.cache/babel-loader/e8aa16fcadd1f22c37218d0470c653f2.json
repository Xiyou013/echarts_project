{"ast":null,"code":"import { reactive, toRefs, watch } from \"vue\";\nexport default {\n  name: \"detailDrawer\",\n  props: {\n    drawer: {\n      type: Boolean,\n      default: false\n    }\n  },\n\n  setup(props, context) {\n    let drawerDetail = reactive({\n      drawerDetail: false\n    });\n    watch(() => props.drawer, (newValue, oldValue) => {\n      console.log(\"---drawer---\", newValue, oldValue);\n      drawerDetail.drawerDetail = newValue;\n      if (newValue || oldValue) return;\n    });\n\n    const handleClose = () => {\n      drawerDetail.drawerDetail = false;\n      context.emit(\"changeDrawer\");\n    };\n\n    return { ...toRefs(drawerDetail),\n      handleClose\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAaA,SAASA,QAAT,EAAmBC,MAAnB,EAA2BC,KAA3B,QAAwC,KAAxC;AAEA,eAAe;EACbC,IAAI,EAAE,cADO;EAEbC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC,OADA;MAENC,OAAO,EAAE;IAFH;EADH,CAFM;;EAQbC,KAAK,CAACL,KAAD,EAAOM,OAAP,EAAgB;IACnB,IAAIC,YAAW,GAAIX,QAAQ,CAAC;MAC1BW,YAAY,EAAE;IADY,CAAD,CAA3B;IAIAT,KAAK,CACH,MAAME,KAAK,CAACC,MADT,EAEH,CAACO,QAAD,EAAWC,QAAX,KAAwB;MACtBC,OAAO,CAACC,GAAR,CAAY,cAAZ,EAA4BH,QAA5B,EAAsCC,QAAtC;MACAF,YAAY,CAACA,YAAb,GAA4BC,QAA5B;MACA,IAAIA,QAAO,IAAKC,QAAhB,EAA0B;IAC5B,CANG,CAAL;;IASA,MAAMG,WAAU,GAAI,MAAM;MACxBL,YAAY,CAACA,YAAb,GAA4B,KAA5B;MACAD,OAAO,CAACO,IAAR,CAAa,cAAb;IACD,CAHD;;IAKA,OAAO,EACL,GAAGhB,MAAM,CAACU,YAAD,CADJ;MAELK;IAFK,CAAP;EAID;;AA/BY,CAAf","names":["reactive","toRefs","watch","name","props","drawer","type","Boolean","default","setup","context","drawerDetail","newValue","oldValue","console","log","handleClose","emit"],"sourceRoot":"","sources":["F:\\hub-code\\echarts_project\\src\\components\\detailDrawer.vue"],"sourcesContent":["<template>\r\n  <el-drawer\r\n    v-model=\"drawerDetail\"\r\n    title=\"人物详情\"\r\n    :close-on-click-modal=\"false\"\r\n    :before-close=\"handleClose\"\r\n    size='600px'\r\n  >\r\n    <span>Hi there!</span>\r\n  </el-drawer>\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs, watch } from \"vue\";\r\n\r\nexport default {\r\n  name: \"detailDrawer\",\r\n  props: {\r\n    drawer: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n  },\r\n  setup(props,context) {\r\n    let drawerDetail = reactive({\r\n      drawerDetail: false,\r\n    });\r\n\r\n    watch(\r\n      () => props.drawer,\r\n      (newValue, oldValue) => {\r\n        console.log(\"---drawer---\", newValue, oldValue);\r\n        drawerDetail.drawerDetail = newValue;\r\n        if (newValue || oldValue) return;\r\n      }\r\n    );\r\n\r\n    const handleClose = () => {\r\n      drawerDetail.drawerDetail = false\r\n      context.emit(\"changeDrawer\");\r\n    };\r\n\r\n    return {\r\n      ...toRefs(drawerDetail),\r\n      handleClose,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n</style>"]},"metadata":{},"sourceType":"module"}